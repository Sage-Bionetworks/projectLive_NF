{
  "team_table": {
    "synapse_id": "syn22279138",
    "team_column": "TeamID",
    "group_column": "Permission List"
  },
  "data_files": {
    "files_table": {
      "synapse_id": "syn22282675"
    },
    "publications_table": {
      "synapse_id": "syn22282673"
    },
    "studies_table": {
      "synapse_id": "syn22282670"
    },
    "tools_table": {
      "synapse_id": "syn22282674"
    }
  },
  "team_filter_column": "fundingAgency",
  "modules": {
    "summary_snapshot": {
      "outputs": {
        "overview_boxes": {
          "box1": {
            "title": "Studies",
            "table": "studies_table",
            "columns": ["studyId"],
            "icon": "university"
          },
          "box2": {
            "title": "Files",
            "table": "files_table",
            "columns": ["id"],
            "icon": "file"
          },
          "box3": {
            "title": "Samples",
            "table": "files_table",
            "columns": ["individualID", "specimenID"],
            "icon": "file"
          },
          "box4": {
            "title": "Publications",
            "table": "publications_table",
            "columns": ["title"],
            "icon": "pencil"
          }
        },
        "consortium_activity": {
          "table": "files_table",
          "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later.",
          "columns": [
            {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
            },
            {
              "name": "consortium",
              "display_name": "Consortium",
              "na_replace": "Not Applicable",
              "type": "character"
            },
            {
              "name": "accessType",
              "display_name": "Access Type",
              "na_replace": "Not Annotated",
              "type": "character"
            }
          ],
          "plot": {
            "x": "Consortium",
            "fill": "Access Type",
            "facet": "Year"
          },
          "tooltips": ["count", "fill"]
        },
        "resources_generated": {
          "table": "files_table",
          "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later.",
          "columns": [
            {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
            },
            {
              "name": "studyName",
              "display_name": "Study Name",
              "type": "character",
              "na_replace": "Not Applicable"
            },
            {
              "name": "dataType",
              "display_name": "Data Type",
              "type": "character",
              "replace_values": {
                "drugScreen": "Drug Screening", 
                "drugCombinationScreen": "Drug Screening",
                "immunofluorescence": "Immunofluorescence",
                "genomicVariants": "Genomic Variants",
                "geneExpression": "Gene Expression",
                "cellularPhysiology": "Cellular Physiology",
                "chromatinActivity": "Chromatin Activity"
              },
              "na_replace": "Other",
              "default_replace": "Other"
            }
          ],
          "plot": {
            "x": "Data Type",
            "fill": "Study Name",
            "facet": "Year"
          },
          "tooltips": ["count", "fill"]
        },
        "merged_table": {            
          "join_column" : "studyName",
          "tables": ["studies_table", "files_table"],
          "columns": [
            ["studyName", "studyLeads"],
            ["studyName", "resourceType", "year", "assay"]
          ]
        },
        "file_upload_timeline":{
          "empty_table_message": "The investigators have not uploaded any files yet. Please check back later.",
          "table": "merged_table",
          "columns": [
            {
              "name": "studyLeads",
              "display_name": "Study Leads",
              "type": "list:character",
              "na_replace": "Pending Annotation"
            },
            {
              "name": "resourceType",
              "display_name": "Resource Type",
              "type": "character",
              "na_replace": "Pending Annotation"
            },
            {
              "name": "year",
              "type": "integer",
              "display_name": "Year" 
            }
          ],
          "plot": {
            "x": "Study Leads",
            "y": "Count",
            "fill": "Resource Type",
            "facet": "Year"
          },
          "tooltips": ["Count", "fill"]
        }
      }
    },
    "file_status": {
       "outputs": {
         "publication_status": {
           "table": "publications_table",
           "columns": [
             {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
             },
             {
              "name": "studyName",
              "display_name": "Study Name",
              "type": "list:character"
             }
           ],
           "plot": {
             "x": "Year",
             "fill": "Study Name"
           },
           "tooltips": ["count", "fill"]
         },
         "publication_disease": {
           "table": "publications_table",
           "columns": [
             {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
             },
             {
              "name": "manifestation",
              "display_name": "Manifestation",
              "type": "list:character",
              "na_replace": "None Listed"
             }
           ],
           "plot": {
             "x": "Year",
             "fill": "Manifestation"
           },
           "tooltips": ["count", "fill"]
         }
       }
    },
    "study_lead": {
       "outputs": {
         "annotation_activity": {
           "table": "merged_table",
           "filter_column": "studyLeads",
           "empty_table_message": "The investigators have not uploaded any files yet. Please check back later.",
           "tooltips": ["Count", "fill"],
           "columns": [
             {
               "name": "studyLeads",
               "display_name": "Study Leads",
               "type": "list:character",
               "na_replace": "Pending Annotation"
             },
             {
               "name": "assay",
               "display_name": "Assay",
               "type": "character",
               "na_replace": "Pending Annotation"
             },
             {
               "name": "year",
               "display_name": "Year",
               "type": "integer"
             }
           ],
           "plot": {
             "x": "Study Leads",
             "y": "Count",
             "fill": "Assay",
             "facet": "Year"
           }
         }
       }
    },
    "study_summary": {
      "outputs": {
        "merged_table": {
          "tables": [
            "studies_table",
            "files_table",
            "tools_table"
          ],
          "filter_column": "studyName",
          "join_column": "studyName",
          "columns": [
            [
              "studyName",
              "studyStatus", 
              "dataStatus", 
              "studyLeads",
              "diseaseFocus", 
              "summary",
              "consortium"
            ],
            [
              "studyName",
              "individualID",
              "specimenID", 
              "assay",
              "id", 
              "resourceType",
              "year", 
              "month",
              "tumorType",
              "species",
              "projectId"
            ],
            ["studyName", "softwareName"]
          ]
        },
        "study_table": {
          "group_columns": [
            "studyName", 
            "studyLeads", 
            "studyStatus",
            "dataStatus",
            "diseaseFocus"
          ],
          "count_columns": [
            "individualID",
            "specimenID",
            "assay", 
            "id", 
            "softwareName"
          ],
          "columns": [
            {"display_name": "Name", "name": "studyName"},
            {"display_name": "Leads", "name":"studyLeads"}, 
            {"display_name": "Study Status", "name":"studyStatus"},
            {"display_name": "Data Status", "name":"dataStatus"}, 
            {"display_name": "Disease Focus", "name":"diseaseFocus"},
            {"display_name": "Individuals", "name":"individualID"}, 
            {"display_name": "Specimens", "name":"specimenID"},
            {"display_name": "Assays", "name":"assay"},
            {"display_name": "Files", "name":"id"},
            {"display_name": "Tools", "name":"softwareName"}
          ],
          "id_column": "Name"
        },
        "data_focus": {
          "columns": [
            {
              "name": "studyName",
              "display_name": "Study Name",
              "type": "character"
            },
            {
              "name": "assay",
              "display_name": "Assays Used",
              "type": "character"
            },
            {
              "name": "resourceType",
              "display_name": "Resource Added",
              "type": "character"
            },
            {
              "name": "species",
              "display_name": "Species Used",
              "type": "character"
            },
            {
              "name": "tumorType",
              "display_name": "Tumor Types Investigated",
              "type": "character"
            }
          ],
          "plot": {
            "x": "Study Name",
            "fill": [
              "Assays Used",
              "Resource Added",
              "Species Used",
              "Tumor Types Investigated"
            ]
          },
          "tooltips": ["count", "fill"],
          "empty_table_message": "No columns selected have data for current Study"
        },
        "study_timeline": {
          "columns": [
            {
              "name": "studyName",
              "display_name": "Study Name",
              "type": "character"
            },
            {
              "name": "resourceType",
              "display_name": "Resource Type",
              "type": "character"
            },
            {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
            },
            {
              "name": "month",
              "display_name": "Month",
              "type": "factor"
            }
            
          ],
          "plot": {
            "x": "Study Name",
            "fill": "Resource Type",
            "facet": ["Year", "Month"]
          },
          "tooltips": ["count", "fill"],
          "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later."
        },
        "publication_status": {
           "table": "publications_table",
           "filter_column": "studyName",
           "columns": [
             {
              "name": "year",
              "display_name": "Year",
              "type": "integer"
             },
             {
              "name": "studyName",
              "display_name": "Study Name",
              "type": "list:character"
             }
           ],
           "plot": {
             "x": "Year",
             "fill": "Study Name"
           },
           "empty_table_message": "This study has no associated publications.",
           "tooltips": ["count", "fill"]
        },
        "study_details": {
          "columns": [
            {
              "name": "projectId",   
              "type": "character",
              "display_name": "Project ID"
            },
            {
              "name": "studyStatus",
              "type": "character",
              "display_name": "Study Status"
            },
            {
              "name": "dataStatus",
              "type": "character",
              "display_name": "Data Status"
            },
            {
              "name": "summary",
              "type": "character"
            },
            {
              "name": "diseaseFocus",
              "type": "list:character",
              "display_name": "Disease Focus"
            }
          ]
        }
      }
    }
  }
}