{
  "header_text": "You are now viewing studies moderated by {data()$selected_group}.Please click on a row in the table below to select a study and view the details. You can also hover over or zoom into parts of the plots to view more information.",
  "study_table": {
    "join_column": "studyId",
    "filter_column": "initiative",
    "tables": {
      "studies": {
        "columns": [
          "studyName",
          "studyId",
          "initiative",
          "studyLeads",
          "studyStatus",
          "dataStatus",
          "diseaseFocus"
        ]
      },
      "files": {
        "columns": [
          "studyId",
          "id",
          "individualID",
          "specimenID",
          "assay"
        ]
      },
      "tools": {
        "columns": [
          "studyId",
          "softwareName"
        ]
      },
      "merged": {
        "columns": [
          {
            "display_name": "Name",
            "name": "studyName"
          },
          {
            "display_name": "Id",
            "name": "studyId"
          },
          {
            "display_name": "Initiative",
            "name": "initiative"
          },
          {
            "display_name": "Leads",
            "name": "studyLeads"
          },
          {
            "display_name": "Study Status",
            "name": "studyStatus"
          },
          {
            "display_name": "Data Status",
            "name": "dataStatus"
          },
          {
            "display_name": "Disease Focus",
            "name": "diseaseFocus"
          },
          {
            "display_name": "Individuals",
            "name": "individualID"
          },
          {
            "display_name": "Specimens",
            "name": "specimenID"
          },
          {
            "display_name": "Assays",
            "name": "assay"
          },
          {
            "display_name": "Files",
            "name": "id"
          },
          {
            "display_name": "Tools",
            "name": "softwareName"
          }
        ]
      }
    }
  },
  "study_name_column": "Name",
  "study_id_column": "Id",
  "study_summary": {
    "table": "studies",
    "filter_column": "studyName",
    "columns": [
      {
        "name": "studyId",
        "display_name": "Unique Study ID"
      },
      {
        "name": "studyStatus",
        "display_name": "Study Status"
      },
      {
        "name": "dataStatus",
        "display_name": "Data Status"
      },
      {
        "name": "summary"
      },
      {
        "name": "diseaseFocus",
        "display_name": "Disease Focus"
      }
    ]
  },
  "study_timeline_plot": {
    "table": "files",
    "filter_column": "studyName",
    "drop_na": true,
    "columns": [
      {
        "name": "studyName",
        "display_name": "Study Name"
      },
      {
        "name": "resourceType",
        "display_name": "Resource Type",
         "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "year",
        "display_name": "Year"
      },
      {
        "name": "month",
        "display_name": "Month"
      }
    ],
    "plot": {
      "x": "Study Name",
      "fill": "Resource Type",
      "facet": ["Year", "Month"],
      "y_axis_text": {
        "size": 10
      }
    },
    "tooltips": ["count","fill"],
    "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later."
  },
  "data_focus_plot": {
    "table": "files",
    "filter_column": "studyName",
    "columns": [
      {
        "name": "studyName",
        "display_name": "Study Name"
      },
      {
        "name": "assay",
        "display_name": "Assays Used",
        "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "resourceType",
        "display_name": "Resource Added",
        "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "species",
        "display_name": "Species Used",
        "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "tumorType",
        "display_name": "Tumor Types Investigated",
        "recode": {"na_replace": "Pending Annotation"}
      }
    ],
    "plot": {
      "x": "Study Name",
      "fill": [
        "Assays Used",
        "Resource Added",
        "Species Used",
        "Tumor Types Investigated"
      ],
      "y_axis_text": {
        "size": 10
      }
    },
    "tooltips": ["count", "fill"],
    "empty_table_message": "The investigators have not uploaded data for this study yet. Please check back later."
  },
  "annotation_activity_plot": {
    "table": "files",
    "filter_column": "studyName",
    "count_column": {
      "name": "Count",
      "complete_columns": ["studyLeads", "year"]
    },
    "columns": [
      {
        "name": "studyLeads",
        "display_name": "Study Leads",
        "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "assay",
        "display_name": "Assay",
        "recode": {"na_replace": "Pending Annotation"}
      },
      {
        "name": "year",
        "display_name": "Year"
      }
    ],
    "plot": {
      "x": "Study Leads",
      "y": "Count",
      "fill": "Assay",
      "facet": "Year",
      "x_axis_text": {
        "size": 10,
        "angle": 90
      },
      "y_axis_text": {
        "size": 10
      }
    },
    "empty_table_message": "The investigators have not uploaded any files yet. Please check back later.",
    "tooltips": [
      "Count",
      "fill"
    ]
  },
  "publication_status_plot": {
    "table": "publications",
    "count_column": {
      "name": "Count",
      "complete_columns": ["year"]
    },
    "filter_column": "studyName",
    "unnest_columns" : "studyName",
    "columns": [
      {
        "name": "year",
        "display_name": "Year"
      },
      {
        "name": "studyName",
        "display_name": "Study Name"
      }
    ],
    "plot": {
      "x": "Year",
      "y": "Count",
      "fill": "Study Name"
    },
    "empty_table_message": "This study has no associated publications yet.",
    "tooltips": ["Count", "fill"]
  },
  "milestone_reporting_plot": {
    "files_table": {
      "name": "files",
      "columns": [
        {
          "name": "fileFormat",
          "display_name": "File Format",
          "recode": {
            "replace_values": {
              "jpg": "image",
              "png": "image",
              "tif": "image",
              "xls": "spreadsheet",
              "xlsx": "spreadsheet",
              "excel": "spreadsheet",
              "docx": "document",
              "pdf": "document"
            },
            "na_replace": "notAnnotated"
          }
        },
        {
          "name": "dataType",
          "display_name": "Data Type",
          "recode": {"na_replace": "notAnnotated"}
        },
        {
          "name": "date",
          "display_name": "Date Created"
        },
        {
          "name": "progressReportNumber",
          "display_name": "Progress Report Number"
        }
      ]
    },
    "incoming_data_table": {
      "name": "incoming_data",
      "unlist_columns": "fileFormat",
      "columns": [
        {
          "name": "fileFormat",
          "display_name": "File Format",
          "recode": {
            "replace_values": {
              "jpg": "image",
              "png": "image",
              "tif": "image",
              "xls": "spreadsheet",
              "xlsx": "spreadsheet",
              "excel": "spreadsheet"
            },
            "na_replace": "notAnnotated"
          }
        },
        {
          "name": "dataType",
          "display_name": "Data Type",
          "recode": {"na_replace": "notAnnotated"}
        },
        {
          "name": "date_uploadestimate",
          "display_name": "Designated Upload Date"
        },
        {
          "name": "progressReportNumber",
          "display_name": "Progress Report Number"
        },
        {
          "name": "estimatedMinNumSamples",
          "display_name": "Expected"
        }
      ]
    },
    "join_columns": ["Data Type", "File Format"],
    "milestone_column": "Progress Report Number",
    "date_created_column": "Date Created",
    "date_estimate_column": "Designated Upload Date",
    "expected_files_column": "Expected",
    "actual_files_column": "Actual"
  }
}
